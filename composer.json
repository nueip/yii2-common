{
    "name": "nueip/yii2-common",
    "description": "Yii 2 Common Lib",
    "keywords": [
        "yii",
        "extension",
        "common"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Gunter Chou",
            "email": "gunter.chou@staff.nueip.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0",
        "yii2tech/ar-softdelete": "^1.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "nueip\\yii2\\common\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}